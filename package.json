    "@big-whale-labs/bwl-eslint-backend": "^1.0.3",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typegoose/typegoose": "^11.0.0",
